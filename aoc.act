import file


def bloopity_bloop(d: str, x):
    bloops = []
    for line in d.splitlines():
        #print(line)
        bloop = x(line)
        bloops.append(bloop)
    return bloops


def read_file(read_file_cap, filename, x):
    f = file.ReadFile(read_file_cap, filename)
    return bloopity_bloop(f.read().decode(), x)
